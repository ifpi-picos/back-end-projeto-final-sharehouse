{
  "name": "modelo",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "lint": "eslint server.js src test",
    "lint:fix": "eslint server.js src test --fix",
    "debug": "node --nolazy --inspect-brk=9229 ./server.js",
    "start": "nodemon ./server.js",
    "dev": "node ./server.js",
    "test:integration": "NODE_ENV=test mocha  --exit --require test/integration/helpers.js test/integration/**/*_spec.js",
    "test:unit": "NODE_ENV=test mocha --require test/unit/helpers.js test/unit/**/*_spec.js",
    "test": "npm run test:unit && npm run test:integration",
    "clean": "rm -rf dist",
    "compile": "npm run clean && babel . -d dist  --ignore ./node_modules --ignore ./test",
    "build": "npm run test && npm run compile"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^5.2.0",
    "sinon": "^7.2.2",
    "supertest": "^3.3.0"
  },
  "dependencies": {
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.17.0",
    "config": "^3.2.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "express-group-routes": "^1.1.0",
    "helmet": "^3.21.1",
    "jsonwebtoken": "^8.3.0",
    "module-alias": "^2.2.2",
    "mongoose": "^5.7.11",
    "multer": "^1.4.2",
    "nodemon": "^1.19.4"
  },
  "_moduleAliases": {
    "@config": "src/config",
    "@controller": "src/controllers",
    "@middleware": "src/middlewares",
    "@model": "src/models",
    "@router": "src/routes",
    "@util": "src/utils",
    "@view": "src/views"
  }
}
